    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "start": "vite --open",
        "build": "tsc && vite build",
        "eslint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
